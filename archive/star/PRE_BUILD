mk_source_dir  $SOURCE_DIRECTORY                                &&
unpack         $SOURCE  ${MD5[0]}                               &&
cd             $SOURCE_DIRECTORY                                &&
if [ "$SELINUX" == "y" ]; then
  unpack  $SOURCE_CACHE/$SOURCE2  ${MD5[1]}  &&
  zcat    $SOURCE_CACHE/$SOURCE2  |  patch  -p1
  # no &&, because their patch is broken
  cd star &&
  patch -p0 Makefile < $SCRIPT_DIRECTORY/Makefile.patch &&
  cd ..
fi
