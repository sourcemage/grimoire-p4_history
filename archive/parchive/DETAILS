           SPELL=parchive
         VERSION=1.1
          SOURCE=par-v${VERSION}.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/par-cmdline
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
        WEB_SITE=http://parchive.sourceforge.net/
