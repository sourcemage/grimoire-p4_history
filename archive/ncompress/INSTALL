cp -f compress   /usr/bin/compress.real   &&
cp -f zcmp       /usr/bin/zcmp.real       &&
cp -f zdiff      /usr/bin/zdiff.real      &&
cp -f zmore      /usr/bin/zmore.real      &&
cp -f compress.1 /usr/man/man1/compress.real.1 &&
cp -f zcmp.1     /usr/man/man1/zcmp.real.1     &&
cp -f zmore.1    /usr/man/man1/zmore.real.1    &&
ln -fs $INSTALL_ROOT/usr/bin/compress.real $INSTALL_ROOT/usr/bin/uncompress.real

