SPELL_LIST=""

if [ $(gaze -q installed gnustep-base) != $VERSION ];
then
    for  spell  in `gaze section gnustep-libs; gaze section gnustep-apps` 
    do
      if [ $spell != gnustep-base ]
      then
	  gaze installed  $spell  &&  SPELL_LIST="$SPELL_LIST $spell"
      fi
    done
    cast -c $SPELL_LIST
fi
