           SPELL=gnustep-base
         VERSION=1.11.1
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=ftp://ftp.gnustep.org/pub/gnustep/core/$SOURCE
        WEB_SITE=http://www.gnustep.org
