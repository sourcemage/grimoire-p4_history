depends          gnustep-make   &&
depends          ffcall         &&
depends          libxml2        &&
optional_depends libxslt        &&
optional_depends openssl        &&
optional_depends gmp            &&

if [ "$GNUSTEP_BUILD_DOC" == "y" ] 
then
    depends tetex           &&
    depends texinfo
fi

if [ "$GNUSTEP_SSL" == "y" ]
then
    depends openssl
fi
