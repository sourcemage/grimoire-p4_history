   SOURCE_URL[1]=${SOURCEFORGE_URL}/$SPELL/${SOURCE}
     SOURCE_HASH=sha512:533cc56b746c4c9a2d70771ecd02668b4e4d82a5baf23764cbef39098bab853895c1e5e6ea0f5c42e63300b9cc8a04a465e8785f186d8311d4abd191e6556fff
