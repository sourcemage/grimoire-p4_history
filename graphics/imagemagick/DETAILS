   SOURCE_URL[0]=ftp://ftp.imagemagick.org/pub/ImageMagick/$SOURCE
   SOURCE_URL[1]=${SOURCEFORGE_URL}/$SPELL/${SOURCE}
     SOURCE_HASH=sha512:b9e9969b9c7eaa29cd1425922fe13cbcfe925848129b9d1a8951e57ed2e139efa301b15d0c24c510ed44171ef22fb1993f32be86c6ce331b4fafb85c40ed609c
