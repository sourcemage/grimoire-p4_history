           SPELL=ImageMagick
         VERSION=5.5.7-13
          SOURCE=${SPELL}-${VERSION}.tar.bz2
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${SPELL}-5.5.7
   SOURCE_URL[0]=${SOURCEFORGE_URL}/imagemagick/${SOURCE}
   SOURCE_URL[1]=ftp://ftp.imagemagick.org/pub/ImageMagick/$SOURCE
          MD5[0]=de3ffdfe73b3dc934f1fb108cc91bcf1
