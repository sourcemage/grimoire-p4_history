            SPELL=xzgv
         VERSION=0.8
           SOURCE=$SPELL-$VERSION.tar.gz
 SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
    SOURCE_URL[0]=ftp://ftp.ibiblio.org/pub/Linux/apps/graphics/viewers/X/$SOURCE
         WEB_SITE=http://rus.members.beeb.net/xzgv.html
          ENTERED=20020406
