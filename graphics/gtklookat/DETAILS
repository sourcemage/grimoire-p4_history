SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/openvrml/$SOURCE
        WEB_SITE=http://openvrml.sourceforge.net/
