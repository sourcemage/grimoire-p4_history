default_install  &&

if [ "$GIMP_DEVEL" == "y" ];then
ln -s $INSTALL_ROOT/usr/bin/gimp-2.3          $INSTALL_ROOT/usr/bin/gimp         &&
ln -s $INSTALL_ROOT/usr/bin/gimp-remote-2.3   $INSTALL_ROOT/usr/bin/gimp-remote  &&
ln -s $INSTALL_ROOT/usr/bin/gimp-console-2.3  $INSTALL_ROOT/usr/bin/gimp-console
fi
