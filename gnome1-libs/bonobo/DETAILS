   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/sources/$SPELL/$BRANCH/$SOURCE
     SOURCE_HASH=sha512:de55e06cc9b2d6e14feb1e0c0b12d9c5a1426a66e42242d82de43fcfef3ff99abab5d3145b988dfe16a9c4c6e2f6fea496dd4f852bb6b806782394dce3d24312
