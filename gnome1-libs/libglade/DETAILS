           SPELL=libglade
         VERSION=0.17
	  BRANCH=`echo $VERSION|cut -d . -f 1,2`
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/sources/$SPELL/$BRANCH/$SOURCE
     SOURCE_HASH=sha512:2ea1ca4cad2323efedd0861eb27df132db7fcedb30c0caacdc9c093f53dc25dde050191e8bc4278b4f446d251b7b2c099e9e5afcb7e1674f871a9337ec180dc2
