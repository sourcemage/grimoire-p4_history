           SPELL=gnome-media
         VERSION=1.2.3
	  BRANCH=`echo $VERSION|cut -d . -f 1,2`
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/sources/$SPELL/$BRANCH/$SOURCE
     SOURCE_HASH=sha512:cc5484cd986de4cadf1cbf3b65bd8366fe12e4b25a5b6aac598c5ad7e2a5447c372ad64ba9d2a89ea7e015db2d1d17be98ac5db0642dfc25f8fa4b2db95352a2
