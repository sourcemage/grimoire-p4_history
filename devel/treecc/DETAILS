           SPELL=treecc
         VERSION=0.3.4
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=${GNU_URL}/dotgnu/pnet/$SOURCE
        WEB_SITE=http://www.gnu.org/projects/dotgnu/
          MD5[0]=1b85ebdcd56ef22d4b6b3e6dbde89ca5
      LICENSE[0]=GPL
         ENTERED=20031105
