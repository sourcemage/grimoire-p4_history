           SPELL=anjuta
         VERSION=1.1.98
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
        WEB_SITE=http://anjuta.sourceforge.net/
         ENTERED=20011124
         UPDATED=20031115
          MD5[0]='b8c6b960b4ad969e223e8b44039305c6'
      LICENSE[0]=GPL
