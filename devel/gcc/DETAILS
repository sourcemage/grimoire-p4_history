           SPELL=gcc
         VERSION=3.3.1
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNU_URL/$SPELL/$SPELL-$VERSION/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnu.org/pub/gnu/$SPELL/$SPELL-$VERSION/$SOURCE
          MD5[0]=eff7c04a90544fff1e875821cb89eb2b
      LICENSE[0]=GPL
        WEB_SITE=http://gcc.gnu.org
         ENTERED=20020525
         UPDATED=20030809
