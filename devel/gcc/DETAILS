   SOURCE_URL[0]=$GNU_URL/$SPELL/$SPELL-$VERSION/$SOURCE
   SOURCE_URL[1]=ftp://gcc.gnu.org/pub/$SPELL/releases/$SPELL-$VERSION/$SOURCE
          MD5[0]='1809aadd5a8c4d5f358c3766176c420b'
