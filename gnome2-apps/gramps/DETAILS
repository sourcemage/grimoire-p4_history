           SPELL=gramps
       BUILD_API=2
. ${SPELL_CONFIG}.p
echo dd=$DEVEL
if [ "${DEVEL}" == "y"  ];then
         VERSION=1.1.1
          MD5[0]=3b89386c082d7d624d36537e601c039b
else
         VERSION=1.0.7
          MD5[0]='021e648a8309ee5b3454ba361ae9d8b1'
fi
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
      LICENSE[0]="GPL"
      MAKE_NJOBS=1
            DOCS="FAQ NEWS README ChangeLog"
