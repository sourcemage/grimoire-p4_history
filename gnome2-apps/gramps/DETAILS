           SPELL=gramps
       BUILD_API=2
. ${SPELL_CONFIG}.p
echo dd=$DEVEL
if [ "${DEVEL}" == "y"  ];then
         VERSION=1.1.0
          MD5[0]=0569360daa644be65ea6f9bddea8c16e
else
         VERSION=1.0.4
          MD5[0]=d4bf9603b28cff2f12d0703a4c17a817
fi
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
      LICENSE[0]="GPL"
      MAKE_NJOBS=1
            DOCS="FAQ NEWS README ChangeLog"
        WEB_SITE=http://gramps.sourceforge.net/
