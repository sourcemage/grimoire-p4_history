           SPELL=dia
         VERSION=0.94
	  BRANCH=`echo $VERSION|cut -d . -f 1,2`
       BUILD_API=2
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/0.94/$SOURCE
     SOURCE_HASH=sha512:cedf19fb38535fe8e342a45e71b39e853a21e5d81ff5b6a71f99c8ba337b397e5d97fa76bf0649a83ad82594f60e41391891c62c86f059beecef5b73948e0397
