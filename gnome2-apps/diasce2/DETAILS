   SOURCE_URL[1]=http://diasce.es.gnome.org/downloads/$SOURCE
     SOURCE_HASH=sha512:66e0f5e01e93c92c7427c713881611028bb8fb910eac01dfae480c59eb96c8731dfd7fd55e2d2e5c40f9b233994227294bf9805db5983400b72a479bd9913c5a
