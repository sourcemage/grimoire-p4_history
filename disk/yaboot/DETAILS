         VERSION=1.3.11
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://penguinppc.org/projects/yaboot/$SOURCE
          MD5[0]=47588c76a1d4081737719a0bd4ac0dc6
