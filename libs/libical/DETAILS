         VERSION=0.24.RC4
          SOURCE=${SPELL}-${VERSION}.tar.gz
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${SPELL}-${VERSION/.RC4/}
   SOURCE_URL[0]=${SOURCEFORGE_URL}/freeassociation/${SOURCE}
          MD5[0]=1b33344288dad035da837c6e6fa60d01
