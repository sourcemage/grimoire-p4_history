           SPELL=stlport
if [[ "$STLPORT_LATEST" == y ]]; then
         VERSION=5.0
          SOURCE=STLport.tar.bz2
   SOURCE_URL[0]=cvs://:pserver:cvs@cvs.stlport.com:/stlport:STLport:STLPORT_${VERSION//./_}
   SOURCE_IGNORE=volatile
else
         VERSION=5.0RC2
          SOURCE=STLport-$VERSION.tar.gz
