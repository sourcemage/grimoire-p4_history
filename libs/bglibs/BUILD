echo  "gcc ${CFLAGS}"                     >conf-cc       &&
echo  "gcc -g -L."                        >conf-ld       &&
echo  "$INSTALL_ROOT/usr/include/bglibs"  >conf-include  &&
echo  "$INSTALL_ROOT/usr/lib/bglibs"      >conf-lib      &&
echo  "$INSTALL_ROOT/usr/bin"             >conf-bin      &&

make
