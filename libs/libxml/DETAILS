           SPELL=libxml
         VERSION=1.8.17
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=ftp://xmlsoft.org/old/$SOURCE
   SOURCE_URL[1]=ftp://speakeasy.rpmfind.net/pub/libxml/old/$SOURCE
   SOURCE_URL[2]=ftp://fr.rpmfind.net/pub/libxml/old/$SOURCE
     SOURCE_HASH=sha512:a9e67f8f7d6c5474f933f3509ec09cf26907dcc90c0740c7216748ac52b09e174891b71fcbe8792b1d7a991c4fbad4008a997144249498972562dd2040fb4d34
