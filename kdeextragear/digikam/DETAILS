   SOURCE_URL[0]=$SOURCEFORGE_URL/digikam/$SOURCE
          MD5[0]=8cc2dd9c76bfd0489d262cf7c1604486
else
         VERSION=0.7
          SOURCE=${SPELL}-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${SPELL}-${VERSION}
   SOURCE_URL[0]=$SOURCEFORGE_URL/digikam/$SOURCE
          MD5[0]=c6ca5e126f7cc4cdfad8be5f42015dfd
fi
