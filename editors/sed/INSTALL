  default_install                                        &&
  ### /bin/sed is "protected" but it is from an "old" install
  ### just incase it has been removed I copy it first 
  ### so the cast will complete.
  if  [ -f ${INSTALL_ROOT}/usr/bin/sed ] ; then
  cp -a ${INSTALL_ROOT}/usr/bin/sed /bin/sed
  fi                                                    
