if  [  "$OO_UPDATED"  = "y"  ];  then
  message  "${MESSAGE_COLOR}Summoning updates...${DEFAULT_COLOR}"  &&
  summon  openssl  python
fi  &&

mk_source_dir	$SOURCE_DIRECTORY  4g	&&
cd		$BUILD_DIRECTORY	&&
unpack		$SOURCE	${MD5[0]}       &&

cd  $SOURCE_DIRECTORY                   &&
patch  -p1  <  $SCRIPT_DIRECTORY/fix-j2sdk+kernel.patch
