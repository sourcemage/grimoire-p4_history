if [ -f /usr/include/jni.h ] ; then
	mv /usr/include/jni.h /usr/include/jni_gcc.h
fi                                     &&
if  [  -z  $JAVA_HOME  ]
then
  export JAVA_HOME="/opt/java/j2sdk/"
fi					&&
mk_source_dir	$SOURCE_DIRECTORY  4g	&&
cd		$BUILD_DIRECTORY	&&
unpack		$SOURCE  ${MD5[0]}	&&
#unpack          $SOURCE2 ${MD5[1]}      &&
export MOZ_PREFIX=/usr
