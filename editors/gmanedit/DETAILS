      LICENSE[0]=GPL
         ENTERED=20011128
         UPDATED=20011128
      MAINTAINER=eric@sandall.us
    MENU_ITEM[0]="GNOME manpage editor"
    MENU_EXEC[0]=${SPELL}
    MENU_DESC[0]=${SHORT}
        KEYWORDS="editors"
           SHORT="gmanedit is a gtk+ manual page editor"

cat << EOF
The GNOME Manpages Editor (gmanedit) is an editor for man pages that
runs on X with GNOME/GTK. It's similar to most common HTML editors
but more easy.
EOF
