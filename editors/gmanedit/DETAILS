           SPELL=gmanedit
         VERSION=0.3.3
       BUILD_API=2
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION.orig
   SOURCE_URL[0]=http://gmanedit.sourceforge.net/files/$SOURCE
     SOURCE_HASH=sha512:13e6991dc0a80e0bb6cbae70c5cef2a6ed0aa208b061fd7389bbdf8403467ce96176ee108db168c292d6e46e0993c1affdd66d2b5dea40137fd1f166c9ac820d
