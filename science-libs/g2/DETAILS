           SPELL=g2
         VERSION=0.70
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://download.sourceforge.net/g2/$SOURCE
          MD5[0]=0ebec80e5ae7e78a87996a6cbd1c8c7e
        WEB_SITE=http://www.sourceforge.net/projects/g2
      LICENSE[0]=LGPL
         ENTERED=20050721
