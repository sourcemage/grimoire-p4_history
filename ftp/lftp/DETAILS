           SPELL=lftp
         VERSION=3.0.7
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://the.wiretapped.net/mirrors/$SPELL/$SOURCE
          MD5[0]=abdc283bfcaf645796d36d0215a975cf
        WEB_SITE=http://lftp.yar.ru/
