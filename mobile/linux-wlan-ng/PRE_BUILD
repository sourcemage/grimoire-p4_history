mk_source_dir  $SOURCE_DIRECTORY  													   &&
mk_source_dir  pcmcia-cs-`installed_version pcmcia-cs` 											   &&
unpack         $SOURCE ${MD5[0]}												           &&
unpack         pcmcia-cs-`installed_version pcmcia-cs`.tar.gz $(echo "$(gaze DETAILS pcmcia-cs)" | fgrep 'MD5[0]' | cut -d= -f2)	   &&
cd /usr/src/pcmcia-cs-`installed_version pcmcia-cs` 											   &&
make oldconfig 																   &&
cd ..         
