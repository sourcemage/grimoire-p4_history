           SPELL=malsync
         VERSION=2.2.0
       BUILD_API=2
          SOURCE=${SPELL}_${VERSION}.src.tar.gz
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${SPELL}
   SOURCE_URL[0]=http://www.tomw.org/malsync/${SOURCE}
     SOURCE_HASH=sha512:09d58a5e29a9d02a01245a3707c6f59d4a7a6e94e911a9147d1e543b1de198fab7fa0270eaa4b1a0aa65474f68a6e4b0fdbf8d35ec0b37b1503f23b7c6d77978
