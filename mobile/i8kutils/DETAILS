           SPELL=i8kutils
         VERSION=1.21
          SOURCE=${SPELL}_${VERSION}.tar.gz
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${SPELL}-${VERSION}
   SOURCE_URL[0]=http://ftp.debian.org/debian/pool/main/i/${SPELL}/${SOURCE}
          MD5[0]=c91998f6eff5612d36f0d19543e065ac
        WEB_SITE=http://people.debian.org/~dz/i8k/
         ENTERED=20030221
