default_pre_build                        &&

cd      ${SOURCE_DIRECTORY}              &&
unpack  ${SOURCE2}  ${MD5[1]}            &&

cd      ${SOURCE_DIRECTORY}              &&
unpack  ${SOURCE3}  ${MD5[2]}            &&

if  [  "${SOURCE4}"  !=  ""  ];  then
  cd      ${SOURCE_DIRECTORY}            &&
  unpack  ${SOURCE4}  ${MD5[3]}
fi

rm  -f  ${SOURCE_DIRECTORY}/Makefile.in  &&
cp  ${SOURCE_DIRECTORY}/src/Makefile.in  ${SOURCE_DIRECTORY}/Makefile.in
