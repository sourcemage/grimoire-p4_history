#!/bin/bash

PROGRAM=/bin/false
RUNLEVEL=3
NEEDS="+local_fs"

. /etc/init.d/smgl_init

start ()
{
  echo "Starting mgetty..."
  DAEMON_START
  evaluate_retval
}

stop  ()
{
  echo "Stopping mgetty..."
  DAEMON_STOP
  evaluate_retval
}

restart ()
{
  echo   "Restarting mgetty..."
  DAEMON_RESTART
  evaluate_retval
}

status ()
{
  DAEMON_STATUS
}
