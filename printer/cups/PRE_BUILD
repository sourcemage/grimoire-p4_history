echo  "This spell conflicts with gnulpr and will cause at least two spells to be dispelled."
if  query  "To see a list of affected spells, enter a 'y': "  y
  then
    gaze  depends  gnulpr
    echo  "gnulpr"

    if  query  "To continue casting enter a 'y': "  y
      then
        for  CASTED_SPELL  in  `gaze  depends  gnulpr`
        do
          if  [  "`gaze  installed  $CASTED_SPELL  |  grep  $CASTED_SPELL`"  =  ""  ]
          then
            dispel  $CASTED_SPELL
          fi
        done

        dispel  gnulpr
      else
	exit
    fi
  else
    if  query  "To continue casting enter a 'y': "  y
      then
        for  CASTED_SPELL  in  `gaze  depends  gnulpr`
        do
          if  [  "`gaze  installed  $CASTED_SPELL  |  grep  $CASTED_SPELL`"  =  ""  ]
          then
            dispel  $CASTED_SPELL
          fi
        done

        dispel  gnulpr
      else
	exit
    fi
fi

default_pre_build
