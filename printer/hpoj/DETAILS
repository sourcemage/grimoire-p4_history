           SPELL=hpoj
         VERSION=0.8
          SOURCE=$SPELL-$VERSION.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://hpoj.sourceforge.net/download/$SOURCE
        WEB_SITE=http://hpoj.sourceforge.net/
         LICENSE=GPL
             MD5=225b5b946d4b11a29a051e04df4c825d
         ENTERED=20020210
