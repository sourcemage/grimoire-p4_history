          SOURCE=ghostscript-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/ghostscript-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/ghostscript/$SOURCE
        WEB_SITE=http://www.ghostscript.com
      LICENSE[0]=AFPL
