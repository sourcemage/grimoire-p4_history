            SPELL=libax25
          VERSION=0.0.11
           SOURCE=${SPELL}-$VERSION.tar.gz
 SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
    SOURCE_URL[0]=${SOURCEFORGE_URL}/ax25/${SOURCE}
         WEB_SITE=http://ax25.sourceforge.net/
