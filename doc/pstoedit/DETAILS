SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://home.t-online.de/home/helga.glunz/wglunz/pstoedit/$SOURCE
        WEB_SITE=http://www.pstoedit.net/pstoedit
