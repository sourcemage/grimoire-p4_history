          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://www.gnucash.org/pub/gnucash/sources/stable/${SOURCE}
   SOURCE_URL[1]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
   SOURCE_URL[2]=ftp://ftp.gnome.org/pub/GNOME/sources/$SPELL/$BRANCH/$SOURCE
      MAINTAINER=jon-sven@frisurf.no
        WEB_SITE=http://www.gnucash.org
      LICENSE[0]=GPL
