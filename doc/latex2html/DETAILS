           SPELL=latex2html
         VERSION=2K.1beta
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$CTAN_URL/support/$SPELL/$SOURCE
   SOURCE_URL[1]=http://www.ctan.org/tex-archive/support/$SPELL/$SOURCE
        WEB_SITE=http://saftsack.fs.uni-bayreuth.de/~latex2ht
