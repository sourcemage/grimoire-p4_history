default_install                                 &&
    
if [ ! -e "${INSTALL_ROOT}/usr/bin/jadetex" ]; then
    ln -s "${INSTALL_ROOT}/usr/bin/tex" "${INSTALL_ROOT}/usr/bin/jadetex"
fi                                              &&

if [ ! -e "${INSTALL_ROOT}/usr/bin/pdfjadetex" ]; then
    ln -s "${INSTALL_ROOT}/usr/bin/pdftex" "${INSTALL_ROOT}/usr/bin/pdfjadetex"
fi
