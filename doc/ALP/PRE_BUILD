mk_source_dir       $SOURCE_DIRECTORY         &&
cd                  $SOURCE_DIRECTORY         &&
cp                  $SOURCE_CACHE/$SOURCE  .  &&
unpack              $SOURCE2                  &&
chown -R root.root  ALP-listings              &&
(
  for  CURR_DIR  in  `find  ALP-listings  -type d`;  do
    chmod 0755  $CURR_DIR
  done

  for  CURR_FILE  in  `find  ALP-listings  -type f`;  do
    chmod 0644  $CURR_FILE
  done
)
