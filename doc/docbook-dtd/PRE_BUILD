mk_source_dir  $SOURCE_DIRECTORY  &&
cd             $SOURCE_DIRECTORY  &&
mkdir          docbook-$VERSION   &&
cd             docbook-$VERSION   &&
unpack_file   &&
cd             $SOURCE_DIRECTORY  &&
mkdir          docbook-4.1.2      &&
cd             docbook-4.1.2      &&
unpack_file 2 &&
cd             $SOURCE_DIRECTORY  &&
mkdir          docbook-4.2        &&
cd             docbook-4.2        &&
unpack_file 3 &&
cd             $SOURCE_DIRECTORY  &&
mkdir          docbook-4.3        &&
cd             docbook-4.3        &&
unpack_file 4 &&
cd             $SOURCE_DIRECTORY
