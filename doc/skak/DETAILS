           SPELL=skak
         VERSION=1.0
          SOURCE=$SPELL.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL
   SOURCE_URL[0]=$CTAN_URL/fonts/$SOURCE
   SOURCE_URL[1]=ftp://ftp.dante.de/tex-archive/fonts/$SOURCE
          MD5[0]=57e4e7f6a44d47155d6b36d72131b6fc
