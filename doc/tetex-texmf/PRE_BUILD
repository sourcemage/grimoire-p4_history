mk_source_dir   $SOURCE_DIRECTORY   &&
cd              $SOURCE_DIRECTORY   &&
unpack          $SOURCE ${MD5[0]}   &&

if [ "$CSTEX" = "y" ]; then
    cd          $BUILD_DIRECTORY    &&
    unpack      $SOURCE2 ${MD5[1]}  &&
    unpack      $SOURCE3 ${MD5[2]}  &&
    unpack      $SOURCE4 ${MD5[3]}  &&
    unpack      $SOURCE5 ${MD5[4]}  &&
    unpack      $SOURCE6 ${MD5[5]}
fi                                  &&

cd              $SOURCE_DIRECTORY
