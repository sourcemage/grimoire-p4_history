           SPELL=winedoc
         VERSION=NA
       BUILD_API=2
          SOURCE=wine-user.ps
         SOURCE2=winelib-user.ps
         SOURCE3=wine-devel.ps
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/$SPELL
   SOURCE_URL[0]=http://www.winehq.org/docs/$SOURCE
  SOURCE2_URL[0]=http://www.winehq.org/docs/$SOURCE2
  SOURCE3_URL[0]=http://www.winehq.org/docs/$SOURCE3
          MD5[0]='d6f9634f946eca9f16f96383c0cdb572'
