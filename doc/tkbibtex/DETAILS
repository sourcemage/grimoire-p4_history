           SPELL=tkbibtex
         VERSION=10
          SOURCE=$SPELL
         SOURCE2=${SPELL}.1
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL
   SOURCE_URL[0]=http://www.cat.csiro.au/cmst/staff/pic/tkbibtex/$SOURCE
  SOURCE2_URL[0]=http://www.cat.csiro.au/cmst/staff/pic/tkbibtex/$SOURCE2
        WEB_SITE=http://www.cat.csiro.au/cmst/staff/pic/tkbibtex.html
