            SPELL=wordnet
          VERSION=1.7.1
           SOURCE=WordNet-$VERSION.tar.gz
 SOURCE_DIRECTORY=$BUILD_DIRECTORY/WordNet-$VERSION
    SOURCE_URL[0]=ftp://ftp.cogsci.princeton.edu/pub/$SPELL/$VERSION/$SOURCE
           MD5[0]=bc3d167059a286dafa27e7818f9773ad
         WEB_SITE=http://www.cogsci.princeton.edu/~wn
          ENTERED=20020611
