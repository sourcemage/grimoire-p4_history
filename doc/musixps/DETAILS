           SPELL=musixps
         VERSION=1.11
          SOURCE=$SPELL-unix.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-unix
   SOURCE_URL[0]=$CTAN_URL/fonts/musixtex/ps-type1/$SOURCE
   SOURCE_URL[1]=ftp://ftp.dante.de/tex-archive/fonts/musixtex/ps-type1/$SOURCE
     SOURCE_HASH=sha512:b5e14caf779425a656602b1df8ed410d0bf042a6a8f191f3ce68077721505652a05f337a047a1461ca6ac0928a05426b1033cb036ff0a8b0b0a2953becf5d299
