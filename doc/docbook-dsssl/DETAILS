           SPELL=docbook-dsssl
         VERSION=1.78
          SOURCE=$SPELL-$VERSION.tar.gz
         SOURCE2=$SPELL-doc-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/docbook/$SOURCE
   SOURCE_URL[1]=http://umn.sourceforge.net/sourceforge/docbook/$SOURCE
  SOURCE2_URL[0]=$SOURCEFORGE_URL/docbook/$SOURCE2
  SOURCE2_URL[1]=http://umn.sourceforge.net/sourceforge/docbook/$SOURCE2
          MD5[0]='f68511313035edc94a475478b73b5b93'
          MD5[1]='46013feb252c3548df76e3c1bec6af98'
