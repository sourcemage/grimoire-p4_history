mk_source_dir  $SOURCE_DIRECTORY    &&
cd             $SOURCE_DIRECTORY    &&
unpack         $SOURCE ${MD5[0]}    &&

if test "$PS" == "y"; then
    mkdir ps
    cd ps
    unpack $SOURCE2 ${MD5[1]}
    cd ..
fi

if test "$PDF" == "y"; then
    mkdir pdf
    cd pdf
    unpack $SOURCE3 ${MD5[2]}
    cd ..
fi

if test "$HTML_MULTI" == "y"; then
    mkdir html
    cd html
    unpack $SOURCE4 ${MD5[3]}
    cd ..
fi

if test "$HTML_SINGLE" == "y"; then
    mkdir html-single
    cd html-single
    unpack $SOURCE5 ${MD5[4]}
    cd ..
fi
