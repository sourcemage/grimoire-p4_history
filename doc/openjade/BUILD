OPTS="--enable-http                                                       \
          --enable-default-catalog=$TRACK_ROOT/etc/sgml/catalog           \
      --enable-default-search-path=$TRACK_ROOT/usr/share/sgml             \
                         --datadir=$INSTALL_ROOT/usr/share/sgml/OpenJade  \
                 --enable-splibdir=$INSTALL_ROOT/usr/lib                  \
      $OPTS"                                                              &&

default_build
