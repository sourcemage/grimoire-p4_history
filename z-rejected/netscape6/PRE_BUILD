if  query  "This is a REJECTED SPELL, to see why enter a 'y': "  y
	then 
	echo  "" 
	echo  ""
	echo  "This spell was rejected due to non-source spell."
	echo  "Do NOT change the installation directory in the "
	echo  "netscape installer or this spell WILL break."
	echo  ""
	echo  ""
	if  query  "To CONTINUE casting enter a 'y': "  y
		then
		echo  ""
		echo  ""
		echo  "Ok, here we go... you're on your own!"
		echo  ""
		echo  ""
	else
		echo  ""
		echo  "Ok, maybe you be braver next time?"
		exit
	fi
fi

