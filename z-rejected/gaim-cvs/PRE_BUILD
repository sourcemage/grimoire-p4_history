. $SCRIPT_DIRECTORY/../librejected
warn_rejected &&
ask_continue_with_rejected &&

if spell_installed gpgme && spell_installed libgcrypt ; then
  mk_source_dir $SOURCE_DIRECTORY &&
  unpack $SOURCE ${MD5[0]} &&
  cd $SOURCE_DIRECTORY/plugins/ &&
  unpack $SOURCE2 ${MD5[1]}
else
  mk_source_dir $SOURCE_DIRECTORY &&
  unpack $SOURCE ${MD5[0]}
fi
