
#!/bin/sh
# /etc/init.d/rcS
#
# Modified by Eric Sandall <eric@sandall.us>
# v1.1  2003-07-20  ES  Modified to be installed by init.d
#
# SMGL-script-version=20030720
#

PATH1="/sbin:/usr/sbin:/usr/local/sbin"
PATH2="/bin:/usr/bin:/usr/local/bin"
PATH3="/usr/games:/usr/local/games"
PATH4="/usr/bin/X11:/."
 PATH="$PATH1:$PATH2:$PATH3:$PATH4"

export  PATH
umask   022
trap    ":" INT QUIT TSTP

for  SCRIPT in /etc/rcS.d/S*; do
  [ -x $SCRIPT ] && $SCRIPT start
done
