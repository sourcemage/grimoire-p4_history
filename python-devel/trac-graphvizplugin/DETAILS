     TRAC_PLUGIN=graphvizplugin
           SPELL=trac-$TRAC_PLUGIN
         VERSION=$(date +%Y%m%d)
   SOURCE_IGNORE='volatile'
          SOURCE="$TRAC_PLUGIN-$VERSION.tar.bz2"
    TRAC_VERSION=0.9
   SOURCE_URL[0]="svn://trac-hacks.swapoff.org/svn/$TRAC_PLUGIN/$TRAC_VERSION/:$SPELL-$VERSION"
    SOURCE_HINTS=old_svn_compat
SOURCE_DIRECTORY="$BUILD_DIRECTORY/$TRAC_PLUGIN-$VERSION"
        WEB_SITE='http://trac-hacks.swapoff.org/wiki/GraphvizPlugin'
      LICENSE[0]='?'
         UPDATED=20051215
       BUILD_API=2
        KEYWORDS="graph python devel"
           SHORT='Inline creation of diagrams for abstract graphs and networks using Graphviz'
cat << EOF
The GraphvizPlugin allows for the inline creation of diagrams for abstract
graphs and networks using the Graphviz programs.

GraphvizPlugin is a plug-in component for Trac.
EOF
