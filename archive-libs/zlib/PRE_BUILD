default_pre_build                                  &&
cd  $SOURCE_DIRECTORY                              &&
patch -p0 < ${SCRIPT_DIRECTORY}/patches/zlib-1.2.2-inftrees.patch &&
patch -p0 < ${SCRIPT_DIRECTORY}/Makefile.patch && 
mk_source_dir $SOURCE_DIRECTORY.static         &&
cp -a $SOURCE_DIRECTORY/* $SOURCE_DIRECTORY.static 
