@f77-cpp-input:
cc1 -E -traditional-cpp -D_LANGUAGE_FORTRAN %(cpp_options) %{E|M|MM:%(cpp_debug_options)} %{!M:%{!MM:%{!E: -o %|.f |\n f771 %|.f %(cc1_options) %{I*} %{!fsyntax-only:%(invoke_as)}}}}

@ratfor:
%{C:%{!E:%eGCC does not support -C without using -E}} %{CC:%{!E:%eGCC does not support -CC without using -E}} ratfor %{C} %{CC} %{v} %{E:%W{o*}} %{!E: %{!pipe:-o %g.f} %i |\n f771 %m.f %(cc1_options) %{I*} %{!fsyntax-only:%(invoke_as)}}

@f77:
%{!M:%{!MM:%{!E:f771 %i %(cc1_options) %{I*} %{!fsyntax-only:%(invoke_as)}}}}

.F:
@f77-cpp-input

.fpp:
@f77-cpp-input

.FPP:
@f77-cpp-input

.r:
@ratfor

.f:
@f77

.for:
@f77

.FOR:
@f77

