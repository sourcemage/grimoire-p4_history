make -C gcc lang.install-common lang.install-man  &&
install -c gcc/f771 ${INSTALL_ROOT}/usr/libexec/gcc/$HOST/$VERSION/  &&
make -C $HOST/libf2c/ install  &&

# specs file stuff
local SPECSDIR=${INSTALL_ROOT}/usr/lib/gcc/$HOST/$VERSION  &&
mkdir -p $SPECSDIR/specs-local                             &&
cp $SCRIPT_DIRECTORY/specs_g77 $SPECSDIR/specs-local
