default_pre_build                                  &&
cd  $SOURCE_DIRECTORY                              &&
patch  -p1  <  ${SCRIPT_DIRECTORY}/freetype.patch

if  [ "$BUILD_ENIGMAIL" = "y" ];  then
  cd  $SOURCE_DIRECTORY/extensions  &&
  unpack  $SOURCE2  ${MD5[1]}       &&
  unpack  $SOURCE3  ${MD5[2]}
fi

