;; BUILD replaces hostname with `hostname`

$TTL    3600

@       IN      SOA     hostname. root.hostname.  (
                                2003091812      ; Serial
                                3600    ; Refresh
                                900     ; Retry
                                3600000 ; Expire
                                3600 )  ; Minimum
        IN      NS      hostname.
1       IN      PTR     hostname.

