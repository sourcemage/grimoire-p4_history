          SOURCE=$SPELL.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL
   SOURCE_URL[0]=http://lcamtuf.coredump.cx/$SOURCE
     SOURCE_HASH=sha512:cd9f51a5b5f0dffaa6d4a802dd08e728ea6f44ceb471c34fed57c772b699b66efa3e8ea6c19aed62e6c9277067c00aa4fe0d4f151ebfd63bf79076f88a27c3bc
