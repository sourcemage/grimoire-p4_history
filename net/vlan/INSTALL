DOCDIR="/usr/share/doc/vlan"        &&
HTMLDIR="$DOCDIR/html"              &&
cp  vconfig    /sbin                &&
cp  vconfig.8  /usr/share/man/man8  &&
mkdir  $DOCDIR                      &&
mkdir  $HTMLDIR                     &&
cp  README      $DOCDIR             &&
cp  CHANGELOG   $DOCDIR             &&
cp  vlan.html   $HTMLDIR            &&
cp  howto.html  $HTMLDIR
