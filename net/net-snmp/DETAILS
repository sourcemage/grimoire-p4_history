           SPELL=net-snmp
         VERSION=4.2.3
          SOURCE=ucd-snmp-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/ucd-snmp-$VERSION
   SOURCE_URL[0]=http://prdownloads.sourceforge.net/net-snmp/$SOURCE
   SOURCE_URL[1]=ftp://ftp.freesnmp.org/mirrors/net-snmp/$SOURCE
