  # Copy binaries
  install  -m 750  server/maradns     $INSTALL_ROOT/usr/sbin
  install  -m 755  tools/askmara      $INSTALL_ROOT/usr/bin
  install  -m 755  tuzona/getzone     $INSTALL_ROOT/usr/bin
  install  -m 750  tuzona/zoneserver  $INSTALL_ROOT/usr/sbin

 install_config_file doc/en/examples/example_mararc $INSTALL_ROOT/etc/mararc  &&
 install_config_file doc/en/examples/example_csv1 $INSTALL_ROOT/etc/maradns/db.example.com && 


 # Copy man pages
  for  i  in  $( seq 1 8 )  ;  do
    install  -m 644  doc/en/man/*.$i  $INSTALL_ROOT/usr/share/man/man$i  >  /dev/null  2>&1
  done 
