#!/bin/bash

PROGRAM=/usr/sbin/zoneserver
RUNLEVEL=3
NEEDS="+remote_fs +network"

. /etc/init.d/smgl_init

start()
{
  echo "Starting zoneserver..."

  getpids $PROGRAM

  if [ -z "$pidlist" ] ; then
    nohup $PROGRAM > /dev/null &

    sleep 1

    getpids $PROGRAM
    [ -n "$pidlist" ] && true || false
    evaluate_retval
  else
    $SET_WCOL
    print_status warning running
  fi

}
