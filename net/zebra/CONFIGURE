config_query_option ZEBRA_VTYSH     'Include integrated vty shell (reccomended)' y '--enable-vtysh' '' &&
config_query_option ZEBRA_IDRP      'Enable IRDP Server support in zebra?' y '--enable-irdp' '--disable-irdp' &&
config_query_option ZEBRA_SNMP      'Enable SNMP support?' y '--enable-snmp' '--disable-snmp' &&
config_query_option ZEBRA_TCPLINK   'Enable TCP/IP socket connection between zebra and protocol daemon?' y '--enable-tcp-zebra' '--disable-tcp-zebra' &&
config_query_option ZEBRA_OSPF_NSSA 'Enable OSPF NSSA option?' y '--enable-nssa' '--disable-nssa' &&
config_query_option ZEBRA_OPAQUE    'Enable OSPF Opaque-LSA support (RFC2370)?' y '--enable-opaque-lsa' '--disable-opaque-lsa' &&
config_query_option ZEBRA_OSPF_TE   'Enable OSPF Traffic Engineering Extensions?' y '--enable-ospf-te' '--disable-ospf-te' &&
config_query_option ZEBRA_TCP_SIG   'Enable TCP MD5 Signature Option (RFC2385)?' y '--enable-tcp-signature' '--disable-tcp-signature'
