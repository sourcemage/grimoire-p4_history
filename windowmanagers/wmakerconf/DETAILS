         VERSION=2.11
       BUILD_API=2
          SOURCE=${SPELL}-${VERSION}.tar.gz
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${SPELL}-${VERSION}
   SOURCE_URL[0]=http://www.starplot.org/wmakerconf/${SOURCE}
        WEB_SITE=http://www.starplot.org/wmakerconf/
         ENTERED=20040910
