         SOURCE2=swig-1.3.17.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
  SOURCE2_URL[0]=$SOURCEFORGE_URL/swig/$SOURCE2
          MD5[0]=acd9c2d2b698d0694b6f5a0f45754684
          MD5[1]=6492e5ac9aa47889e0e5fdeae53b8fad
