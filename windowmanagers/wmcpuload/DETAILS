           SPELL=wmcpuload
         VERSION=1.0.1
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://www.sh.rim.or.jp/~ssato/src/$SOURCE
   SOURCE_URL[1]=http://download.sourcemage.org/mirror/$SOURCE
        WEB_SITE=http://www.sh.rim.or.jp/~ssato/dockapp/index.shtml#wmcpuload
          MD5[0]=d22da688f050b58c39702f83a005a829
