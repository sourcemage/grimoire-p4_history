#!/bin/bash

PROGRAM=/usr/sbin/wdm
RUNLEVEL=5
PROVIDES=display_manager

. /etc/init.d/smgl_init

# overrides default start function, ensuring
# that wdm releases control back to init system.
start() {
  echo "Starting wdm..."
  if [ -e /usr/bin/wdm ]; then
	  /usr/bin/wdm &
  else
    print_status failure
    exit 1
  fi
}
