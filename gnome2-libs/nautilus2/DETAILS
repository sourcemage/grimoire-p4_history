          SOURCE=nautilus-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/nautilus-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/nautilus/$BRANCH/$SOURCE
          MD5[0]='b5090f4c86b6a0740ebfbf723f43f216'
