           SPELL=metacity
         VERSION=2.6.2
          BRANCH=`echo $VERSION|cut -d . -f 1,2` 
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
          MD5[0]='6004f5c6c6759f7022c1ff6ca1146b09'
      LICENSE[0]=GPL
        WEB_SITE=http://people.redhat.com/~hp/metacity
      MAINTAINER=rcook@wyrms.net
         ENTERED=20020509
