          SOURCE=gnome-python-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/gnome-python-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/gnome-python/$BRANCH/$SOURCE
