#!/bin/bash

DESCRIPTION="security fix for uselib local root compromise"
SHORT="security fix for uselib local root compromise"
HELP="Vulnerability Local User root compromise in uselib()

This affects linux versions equal to and prior to the patches provided in the 2.4.x and 2.6.x versions.
"
VERSIONS="2.6.10
2.6.9
2.4.28"

MD5S[0]=cd0a7533c43364e377348613c6d76bb7
MD5S[1]=49f07af6035cd8f30a22992a291ae799
MD5S[2]=29e531cdd3f2effce5e31a1f2afb5b5d

if [[ "$1" == "CONFIG" ]]; then
echo "
SOURCE${COUNTER}='1115_sys-uselib-fix-${!PATCH}.patch'
SOURCE${COUNTER}_URL='http://smgl.positivism.org/1115_sys-uselib-fix-${!PATCH}.patch'" >> ${SPELL_CONFIG}.DETAILS
source $SCRIPT_DIRECTORY/MD5PATCH
fi
