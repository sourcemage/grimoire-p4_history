#!/bin/bash

DESCRIPTION="Con Kolivas Patchset"
SHORT="These are patches designed to improve system responsiveness, with emphasis on desktop pcs"
HELP="NOTE: This is suitable for ordinary not very stressed desktops.
ck3 Contains: O(1)B,PE,LL,AA,RL2,SM,XFS,ACPI,Tuning

alsa got removed - use the 

*DO NOT USE COMRESSED CACHING WITH PREEMPT - FS UNSAFE*

http://kernel.kolivas.net"

VERSIONS="ck3_2.4.20"
MD5S[0]=a288eb4a8ff5909ec31e3fba4079b455

if [[ "$1" == "CONFIG" ]]; then
echo "
SOURCE${COUNTER}=${!PATCH}.patch.bz2
SOURCE${COUNTER}_URL=http://members.optusnet.com.au/ckolivas/kernel/\$SOURCE${COUNTER}" >> ${SPELL_CONFIG}.DETAILS

source $SCRIPT_DIRECTORY/MD5PATCH
fi
