#!/bin/bash
#
#

DESCRIPTION="openMosix is a GNU/Linux kernel extension for single-image clustering"
SHORT="Kernel extension for single-image clustering"
HELP="Adds single-image clustering capability to the kernel
http://openmosix.sourceforge.net/

This patch *possibly* conflicts xfs, preempt, low lat, ck, ptrace and maybe more
help in merging some of this would be welcome
"
VERSIONS="2.4.22-2.bz2
2.4.22-1.bz2
2.4.21-1.bz2
2.4.20-3.gz
2.4.19-7.gz"
MD5S[0]=1cccdabd2b21f384b29fcf9724557e86
MD5S[1]=8e102a556d8d1a9ac269c2ed25a005a3
MD5S[2]=45c783edcb53b18a394e1f03de1f77b4
MD5S[3]=6717a5842d005e209e1f70044c2961a1
MD5S[4]=775c4c3123db4b9c56927a2de35af0f4

if [[ "$1" == "CONFIG" ]]; then
echo "
SOURCE${COUNTER}=openMosix-${!PATCH}
SOURCE${COUNTER}_URL=\${SOURCEFORGE_URL}/openmosix/\$SOURCE${COUNTER}" >> ${SPELL_CONFIG}.DETAILS
source $SCRIPT_DIRECTORY/MD5PATCH
fi
