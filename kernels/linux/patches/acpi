SOURCE${COUNTER}=acpi-${!PATCH}.diff.gz
SOURCE${COUNTER}_URL=\$SOURCEFORGE_URL/acpi/\$SOURCE${COUNTER}" >> ${SPELL_CONFIG}.DETAILS
source $SCRIPT_DIRECTORY/MD5PATCH
fi
