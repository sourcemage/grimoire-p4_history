#!/bin/bash
# 
#

DESCRIPTION="Medley Software RAID"
SHORT="Medley Software RAID (Silicon Image 3112 and others)"
HELP="Medley Software RAID (Silicon Image 3112 and others)"
VERSIONS="2.4.25"
MD5S[0]=7f85e1dc0e3f65b0b504e60d2e859ff5

if [[ "$1" == "CONFIG" ]]; then
echo "
SOURCE${COUNTER}=medleyraid_v7_for_${!PATCH}.patch
SOURCE${COUNTER}_URL=http://www.infowares.com/linux/patches/\$SOURCE${COUNTER}" >> ${SPELL_CONFIG}.DETAILS
source $SCRIPT_DIRECTORY/MD5PATCH
fi
