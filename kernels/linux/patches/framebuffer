#!/bin/bash
# 
#

DESCRIPTION="Framebuffer logo"
SHORT="The SourceMage raven as framebuffer logo"
HELP="The SourceMage raven as framebuffer logo"
VERSIONS="raven-0.1"
MD5S[0]=9b5d6ec6b5cef9727abb2c9e3b55a057


if [[ "$1" == "CONFIG" ]]; then
echo "
SOURCE${COUNTER}=framebuffer-${!PATCH}.patch.bz2
SOURCE${COUNTER}_URL=http://download.sourcemage.org/distro/kernel/\$SOURCE${COUNTER}" >> ${SPELL_CONFIG}.DETAILS
source $SCRIPT_DIRECTORY/MD5PATCH
fi
