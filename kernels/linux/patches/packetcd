VERSIONS="2.6.3
2.6.2-rc2
2.6.1-rc1-2
2.6.1-rc1"

MD5S[0]=5714ea582a87acb1015df2f913f45569
MD5S[1]=396af9fe54b6be64a705c3706c83d83a
MD5S[2]=ba907f3ca25bf8edcdd90f3701e7db5d
