if [[ "$1" == "CONFIG" ]]; then
echo "
SOURCE${COUNTER}=packet-${!PATCH}.patch.bz2
SOURCE${COUNTER}_URL=http://w1.894.telia.com/~u89404340/patches/packet/2.6/\$SOURCE${COUNTER}" >> ${SPELL_CONFIG}.DETAILS
source $SCRIPT_DIRECTORY/MD5PATCH
fi
