#!/bin/bash

DESCRIPTION="Toshiba ACPI extras"
SHORT="Toshiba laptops specific ACPI"
HELP="Provides ACPI access to LCD brightness, video out switching, fan control and function keys. ***REQUIRES*** the ACPI patch"
VERSIONS="0.13-linux_2.5.26
0.13-linux_2.4.19-acpi_20020821
0.13-linux_2.4.18"
MD5S[0]=3762eeb68e4558a3437d45251617cf91
MD5S[1]=bf5e2b03b10c09078b4d2a346577aefd
MD5S[2]=cbc80f2a00a7691acaf27ea3f0a367dc

if [[ "$1" == "CONFIG" ]]; then
echo "
SOURCE${COUNTER}=toshiba_acpi_${!PATCH}.patch
SOURCE${COUNTER}_URL=http://memebeam.org/free-software/toshiba_acpi/\$SOURCE${COUNTER}" >> ${SPELL_CONFIG}.DETAILS
source $SCRIPT_DIRECTORY/MD5PATCH
fi
