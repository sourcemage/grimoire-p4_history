LATEST_2_2="2.2.25"
LATEST_2_2_PRE="${LATEST_2_2}"

LATEST_2_0="2.0.39"
LATEST_2_0_PRE="2.0.40-rc6"

LATEST_2_4_AC="2.4.21-rc8-ac1"
LATEST_2_5_AC="2.5.69-ac1"

LATEST_2_5_DJ="2.5.69-dj1"

LATEST_2_6_TEST="2.6.0-test6"
