#!/bin/bash
#
# I used the same Order as on http://www.kernel.org/
#
# If some release is not available, use a other version as fallback
# 
# The only thing that matters is the String itself, the script parses all
# necessary information from that
#

LATEST_2_4="2.4.20"
LATEST_2_4_PRE="2.4.21-pre4"

LATEST_2_5="2.5.60"
LATEST_2_5_PRE="${LATEST_2_5}"

LATEST_2_2="2.2.23"
LATEST_2_2_PRE="2.2.24-rc2"

LATEST_2_0="2.0.39"
LATEST_2_0_PRE="2.0.40-rc6"

LATEST_2_4_AC="2.4.21-pre4-ac4"
LATEST_2_5_AC="2.5.50-ac1"

LATEST_2_5_DJ="2.5.60-dj2"
