mk_source_dir $SOURCE_DIRECTORY &&
cd $SOURCE_DIRECTORY &&
unpack $SOURCE ${MD5[0]} &&
unpack $SOURCE1 ${MD5[1]} &&
cd ipw2100-$VERSION


if [ $PATCH24 == 'y' ]; then
	if [ -f ${SCRIPT_DIRECTORY}/patches-2.4/$PATCHFILE ]; then 
		patch -p1 <${SCRIPT_DIRECTORY}/patches-2.4/$PATCHFILE
	else
		message "${PROBLEM_COLOR}There isn't a 2.4 patch out yet${DEFAULT_COLOR}"
	fi
fi

cd ../
