####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: psi.pro  /usr/share/qt/mkspecs/default/qmake.conf configure.pri
	$(QMAKE) -o Makefile psi.pro
qmake: 
	@$(QMAKE) -o Makefile psi.pro

dist: 
	@mkdir -p .tmp/psi && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/psi/ && $(COPY_FILE) --parents psi_jp.ts psi_de.ts psi_zh.ts psi_pl.ts psi_se.ts psi_cz.ts psi_fr.ts psi_es.ts psi_ru.ts psi_da.ts psi_pt.ts psi_nl.ts psi_ar.ts psi_ptbr.ts psi_it.ts psi_sr.ts .tmp/psi/ && ( cd `dirname .tmp/psi` && $(TAR) psi.tar psi && $(GZIP) psi.tar ) && $(MOVE) `dirname .tmp/psi`/psi.tar.gz . && $(DEL_FILE) -r .tmp/psi

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
