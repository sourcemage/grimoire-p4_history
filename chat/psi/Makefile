####### Install

install_libfiles: 
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/psi/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/psi/
		-$(COPY_DIR) "../image" "$(INSTALL_ROOT)/usr/share/psi/"
	-$(COPY_DIR) "../iconsets" "$(INSTALL_ROOT)/usr/share/psi/"
	-$(COPY_DIR) "../sound" "$(INSTALL_ROOT)/usr/share/psi/"
	-$(COPY_DIR) "../certs" "$(INSTALL_ROOT)/usr/share/psi/"
	-$(COPY_FILE) "../README" "$(INSTALL_ROOT)/usr/share/psi/"
	-$(COPY_FILE) "../COPYING" "$(INSTALL_ROOT)/usr/share/psi/"


uninstall_libfiles: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/image" -$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/iconsets" -$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/sound" -$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/certs" -$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/README" -$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/COPYING"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/share/psi/"


install_target: 
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/bin/
	$(DEL_FILE) "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"
	-$(COPY) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"
	-"/bin/true" "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/bin/"


install: all install_libfiles install_target 

uninstall: uninstall_libfiles uninstall_target 

