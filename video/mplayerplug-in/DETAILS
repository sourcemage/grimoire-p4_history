          SOURCE=${SPELL}-${VERSION}.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
          MD5[0]=9c9a733ec6a3422af3bd2da025b71242 
        WEB_SITE=http://$SPELL.sourceforge.net/
