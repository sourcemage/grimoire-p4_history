if  [  "$KDE_DEBUG"  =  "y"  ];  then
  OPTS="$OPTS  --enable-debug"
else
  OPTS="$OPTS  --disable-debug"
fi  &&

if  [  "$KDE_FINAL"  =  "y"  ];  then
  OPTS="$OPTS  --enable-final"
else
  OPTS="$OPTS  --disable-final"
fi  &&

#if  [  "$KDE_DNOTIFY"  =  "y"  ];  then
#  OPTS="$OPTS  --enable-dnotify"
#else
#  OPTS="$OPTS  --disable-dnotify"
#fi  &&

OPTS="$OPTS  --with-distribution"  &&

default_build
