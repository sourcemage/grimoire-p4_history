local  i  ii     &&
. $SPELL_CONFIG  &&

mk_source_dir        $SOURCE_DIRECTORY  &&
cd                   $SOURCE_DIRECTORY  &&

if  !  [[  -z  $SOURCE  ]]
then
  unpack  $SOURCE  ${MD5[0]}  &&
  i=2                         &&
  ii=SOURCE$i                 &&
  while  !  [[  -z  ${!ii}  ]]
  do
    unpack  ${!ii}  ${MD5[$i-1]}  &&
    i=$[$i+1]                     &&
    ii=SOURCE$i
  done
fi
