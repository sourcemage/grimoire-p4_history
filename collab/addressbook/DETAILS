          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://ftp.novell.com/pub/forge/ifolder/source/daily/$SOURCE
     SOURCE_HASH=sha512:cafc115d94182d9564c746777de38daa78bfb6937a6d2e1fae2077e9c26193db2bfcb69729d96fe39a4bbfd4eccf1f9ff6832a1c30f0dec48ed43da3db848fb9
