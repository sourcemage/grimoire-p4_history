# $Id: DEPENDS,v 1.2 2002/10/03 00:14:27 sergeyli Exp $
requires webserver &&

depends openssl &&
depends perl &&

optional_depends db '--enable-db' '--disable-db' 'for Berkeley DB support' &&
optional_depends openldap '' '' 'for LDAP support' &&
optional_depends sendmail '--enable-sendmail' '--disable-sendmail' 'for sendmail support'
