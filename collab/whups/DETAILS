           SPELL=whups
         VERSION=${VERSION:-1.0}
          SOURCE=$SPELL-${VERSION}${SUFFIX:-.tar.gz}
      LICENSE[0]=GPL
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-${VERSION%-*}
   SOURCE_URL[0]=${SOURCE_URL[0]:-ftp://ftp.horde.org/pub/$SPELL/$SOURCE}
        WEB_SITE=http://www.horde.org/$SPELL/
         ENTERED=20031027
         UPDATED=20031027
      MAINTAINER=sergey@sourcemage.org
           SHORT='A project management system based on Horde framework'

[ -z $FORCE_DOWNLOAD ] &&
          MD5[0]='IGNORE'

cat << EOF
The Web Horde User Problem Solver, besides being a contrived acronym,
is a project to bring together bug tracking, a web interface to source
control, and a FAQ system under one framework.
WARNING: Beta quality software!
EOF
