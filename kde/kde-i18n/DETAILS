# the remaining details of this spell are provided by PREPARE,
# and stored in $CONFIG_CACHE/kde.languages
           SPELL=kde-i18n
if [ "$DEVEL" == "yes" ] ;then
         VERSION=3.2.92
            SITE=$KDE_URL/unstable/$VERSION/src/$SPELL
else
         VERSION=3.3.0
            SITE=$KDE_URL/stable/${VERSION/\.0/}/src/$SPELL
fi
       BUILD_API=2

SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
        WEB_SITE=http://www.kde.org
         ENTERED=20020817
         UPDATED=20040116
      LICENSE[0]=GPL
           SHORT="Translated KDE messages."
cat << EOF
kde-i18n contains translated KDE messages.
EOF
