if  spell_installed  Xft1  ;  then
  dispel  Xft1;
  touch  /tmp/qt-x11.xft1.suspend
fi

if  spell_installed  Xft2  ;  then
  dispel  Xft2;
  touch  /tmp/qt-x11.xft2.suspend
fi

mk_source_dir  $SOURCE_DIRECTORY  &&
cd             $SOURCE_DIRECTORY  &&
unpack         $SOURCE
mv             $SPELL-$VERSION/* .

