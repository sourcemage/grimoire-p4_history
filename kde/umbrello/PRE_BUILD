mk_source_dir  $SOURCE_DIRECTORY  &&
unpack         $SOURCE  ${MD5[0]} &&
TEST=`flex --version`

if [[ $TEST = 'flex 2.5.27' ]]; then
	cd $SOURCE_DIRECTORY/uml/classparser/ &&
	patch  -p0  <  $SCRIPT_DIRECTORY/umbrello-flex-2.5.27.patch
fi

if [[ $TEST = 'flex 2.5.31' ]]; then
	cd $SOURCE_DIRECTORY/uml/classparser/ &&
	patch  -p0  <  $SCRIPT_DIRECTORY/umbrello-flex-2.5.31.patch
fi
