if [[ $(installed_version Python) == 2.2 ]] && spell_installed tk &&
spell_installed tcl; then

   LIBPYTHON="-L/usr/lib/python`installed_version Python | cut -c -3`/config/ -lpython`installed_version Python | cut -c -3`" 
   LIBPYTHON="$LIBPYTHON -ltcl`installed_version tcl | cut -c -3`" 
   LIBPYTHON="$LIBPYTHON -ltk`installed_version tk | cut -c -3`" 

   export LIBPYTHON
fi &&
default_pre_build

