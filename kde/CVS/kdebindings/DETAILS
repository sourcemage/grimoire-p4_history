           SPELL=kdebindings
         VERSION=KDE_3_1_BRANCH
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=cvs://:pserver:anonymous@anoncvs.kde.org:/home/kde:$SPELL:$VERSION
  FORCE_DOWNLOAD=on
        WEB_SITE=http://www.kde.org
         UPDATED=20020528
