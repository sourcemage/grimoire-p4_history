           SPELL=xmms-kde
         VERSION=0.6.5
          SOURCE=$SPELL-$VERSION.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://prdownloads.sourceforge.net/$SPELL/$SOURCE
        WEB_SITE=http://xmms-kde.sourceforge.net
         ENTERED=20020210
