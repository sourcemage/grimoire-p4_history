OPTS="$EVIDENCE_ICONS      \
      $EVIDENCE_ATTRS      \
      $EVIDENCE_ACLS       \
      $OPTS"               &&

CFLAGS="$CFLAGS -I/usr/include/libgnomeui-2.0 \
                -I/usr/include/libgnomeui-2.0/libgnomeui \
                -I/usr/include/libgnomecanvas-2.0 \
                -I/usr/include/libart-2.0/"  &&
CXXFLAGS="$CXXFLAGS -I/usr/include/qt "  &&

default_build
