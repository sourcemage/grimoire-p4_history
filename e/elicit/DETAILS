         VERSION=$(date +%Y%m%d)
else
         VERSION=cvs
fi
          SOURCE=$SPELL-cvs.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-cvs
   SOURCE_URL[0]=cvs://:pserver:anonymous@cvs.sourceforge.net:/cvsroot/enlightenment:e17/apps/elicit
  FORCE_DOWNLOAD="on"
          MD5[0]=IGNORE
        WEB_SITE=http://enlightenment.org/Applications/Elicit/
