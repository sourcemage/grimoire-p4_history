         VERSION=1.0.4
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=$SOURCEFORGE_URL/enlightenment/$SOURCE
          MD5[0]=095bcbc32ff1f3f384e3265c1355571d
else
         VERSION=cvs
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=cvs://:pserver:anonymous@cvs.sourceforge.net:/cvsroot/enlightenment:e17/libs/imlib2_loaders
          MD5[0]=IGNORE
fi
