           SPELL=entrance
if  [  "$ENTRANCE_CVS_AUTOUPDATE"  ==  "y"  ];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=cvs
fi
          SOURCE=$SPELL-cvs.tar.bz2
  FORCE_DOWNLOAD=on
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-cvs
   SOURCE_URL[0]=cvs://:pserver:anonymous@thinktux.net:/root:e17/apps/$SPELL
   SOURCE_URL[1]=cvs://:pserver:anonymous@cvs.sourceforge.net:/cvsroot/enlightenment:e17/apps/$SPELL
   SOURCE_IGNORE=volatile
      LICENSE[0]=BSD
        WEB_SITE=http://xcomputerman.com/pages/entrance.html
