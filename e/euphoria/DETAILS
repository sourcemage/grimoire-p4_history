           SPELL=euphoria
if  [  "$EUPHORIA_CVS_AUTOUPDATE"  ==  "y"  ];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=cvs
fi
          SOURCE=$SPELL-cvs.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-cvs
   SOURCE_URL[0]=cvs://:pserver:anonymous@cvs.sourceforge.net:/cvsroot/enlightenment:e17/apps/$SPELL
  FORCE_DOWNLOAD="on"
   SOURCE_IGNORE=volatile
      LICENSE[0]=BSD
        WEB_SITE=http://www.enlightenment.org/
