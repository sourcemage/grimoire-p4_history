           SPELL=Date-Parse
         VERSION=1.1301
          SOURCE=TimeDate-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/TimeDate-$VERSION
   SOURCE_URL[0]=http://www.cpan.org/authors/id/GBARR/$SOURCE
          MD5[0]=aae3bca98d9d8603e4993427e1466532
