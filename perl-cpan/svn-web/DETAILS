           SPELL=svn-web
         VERSION=0.47
          SOURCE=SVN-Web-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/SVN-Web-$VERSION
   SOURCE_URL[0]=http://www.cpan.org/authors/id/N/NI/NIKC/$SOURCE
     SOURCE_HASH=sha512:aa03ac566d0922174f58a4ab722305d0908863a2fd5be540ecd17fabc8200d2fd18451127d6133fa91bfb8fe95866b9f71b19785abeaca1068ff0ccc5f7b595e
