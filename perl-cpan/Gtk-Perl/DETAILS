           SPELL=Gtk-Perl
         VERSION=0.7008
          SOURCE=Gtk-Perl-0.7008.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/Gtk-Perl-0.7008
   SOURCE_URL[0]=http://www.cpan.org/authors/id/L/LU/LUPUS/Gtk-Perl-0.7008.tar.gz
