           SPELL=MP3-Tag
         VERSION=0.40
          SOURCE=tagged-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/tagged-$VERSION
      MAKE_NJOBS=1
   SOURCE_URL[0]=http://www.cpan.org/authors/id/T/TH/THOGEE/$SOURCE
          MD5[0]="89d9163c883cc75ee65dfdef277c60a2"
