mk_source_dir $SOURCE_DIRECTORY  &&
unpack_file '' &&
if [ "$LICQ_CVS" == "n" ]; then
        cd $SOURCE_DIRECTORY  &&
        patch -p0 < $SCRIPT_DIRECTORY/licq-email.patch
fi   &&
if [ "$LICQ_CVS" == "y" ]; then
	cd $SOURCE_DIRECTORY			 &&
	unpack		  $SOURCE2 ${MD5[1]} &&
	cd $SOURCE_DIRECTORY			 &&
	unpack		  $SOURCE3 ${MD5[2]} &&
	rm -rf $SOURCE_DIRECTORY/plugins &&
	mv $SOURCE_DIRECTORY/$SPELL-plugins-cvs $SOURCE_DIRECTORY/plugins
fi
if [ "$XOSD" == "y" ]; then
	cd $SOURCE_DIRECTORY/plugins	 &&
	unpack		  $SOURCE4 ${MD5[3]}
fi
