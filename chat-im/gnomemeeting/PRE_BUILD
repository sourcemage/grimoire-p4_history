if [ "$CVS" == "y" ]; then
   cd $SOURCE_DIRECTORY
   export ACLOCAL_PATH="$ACLOCAL_PATH:/usr/share/aclocal:/usr/share/aclocal-1.9:/usr/share/aclocal-1.8:/usr/share/aclocal-1.7:/usr/share/aclocal-1.4"
   ./autogen.sh --prefix=/usr     \
                --sysconfdir=/etc \
                --localstate=/var \
                $OPTS             
fi
