           SPELL=jabberd
         VERSION=2.0s11
          SOURCE=$SPELL-$VERSION.tar.gz
          SOURCE_GPG="gurus.gpg:${SOURCE}.sig"
      LICENSE[0]=GPL
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://jabberstudio.2nw.net/jabberd2/$SOURCE
