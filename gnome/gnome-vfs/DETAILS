           SPELL=gnome-vfs
         VERSION=1.0.4
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/stable/sources/$SPELL/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/stable/sources/$SPELL/$SOURCE
      MAINTAINER=jon-sven@risurf.no
        WEB_SITE=http://www.gnome.org
         ENTERED=20010922
         UPDATED=20020124
           SHORT="gnome-vfs provides the Gnome Virtual File System."
cat << EOF
gnome-vfs
This is the GNOME Virtual File System.
GNOME VFS is currently used as one of the foundations of the Nautilus
file manager.
EOF
