           SPELL=gnome-vfs
         VERSION=1.0.5
	  BRANCH=`echo $VERSION|cut -d . -f 1,2`
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/sources/$SPELL/$BRANCH/$SOURCE
      MAINTAINER=jon-sven@risurf.no
        WEB_SITE=http://www.gnome.org
      LICENSE[0]=GPL
      LICENSE[1]=LGPL
         ENTERED=20010922
         UPDATED=20020330
           SHORT="gnome-vfs provides the Gnome Virtual File System."
cat << EOF
gnome-vfs
This is the GNOME Virtual File System.
GNOME VFS is currently used as one of the foundations of the Nautilus
file manager.
EOF
