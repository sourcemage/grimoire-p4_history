           SPELL=libunicode
         VERSION=0.4
      #  VERSION=0.7
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/unstable/sources/$SPELL/$SOURCE
 # SOURCE_URL[0]=http://prdownloads.sourceforge.net/$SPELL/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/unstable/sources/$SPELL/$SOURCE
      MAINTAINER=jon-sven@frisurf.no
     #  WEB_SITE=http://sourceforge.net/projects/libunicode
        WEB_SITE=unknown
      LICENSE[0]=GPL
