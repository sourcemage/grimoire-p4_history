           SPELL=gnome-core
         VERSION=1.4.0.8
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/stable/sources/$SPELL/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/stable/sources/$SPELL/$SOURCE
      MAINTAINER=jon-sven@frisurf.no
        WEB_SITE=http://www.gnome.org
         ENTERED=20010922
         UPDATED=20020505
           SHORT="gnome-core contains common files for Gnome core apps."
cat << EOF
Gnome is the "GNU Network Object Model Environment"
It is a project to build a complete, user-friendly desktop based
entirely on free software.
This package includes some files that are shared between several Gnome apps. 
EOF
