           SPELL=eel2
         VERSION=2.2.4
	  BRANCH=`echo $VERSION|cut -d . -f 1,2`
          SOURCE=eel-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/eel-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/eel/$BRANCH/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/sources/eel/$BRANCH/$SOURCE
          MD5[0]=3cd4404050df9dab51a20148df0f06e4
