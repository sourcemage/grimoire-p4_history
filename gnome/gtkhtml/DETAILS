           SPELL=gtkhtml
         VERSION=1.0.2
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/unstable/sources/$SPELL/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/unstable/sources/$SPELL/$SOURCE
   SOURCE_URL[2]=ftp://ftp.ximian.com/pub/source/evolution/${SOURCE/bz2/gz}
      MAINTAINER=jon-sven@frisurf.no
        WEB_SITE=unknown
         ENTERED=20011028
         UPDATED=20020326
           SHORT="gtkhtml is a html rendering/editing library."
cat  <<  EOF
GtkHTML is a HTML rendering/editing library.  It is not designed to be
the ultimate HTML browser/editor; instead, it is designed to be easily
embedded into applications that require lightweight HTML functionality.
EOF
