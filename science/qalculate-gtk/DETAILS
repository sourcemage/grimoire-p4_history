          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/qalculate/$SOURCE
          MD5[0]=dfce01fe94e07239f5786de08c30c8d7
