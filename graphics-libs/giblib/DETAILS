           SPELL=giblib
         VERSION=1.2.4
       BUILD_API=2
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://linuxbrit.net/downloads/$SOURCE
        WEB_SITE=http://linuxbrit.net/giblib
         ENTERED=20011101
         UPDATED=20020318
          MD5[0]=d2743c670c9c1eeb3bd5dde0d9272101
