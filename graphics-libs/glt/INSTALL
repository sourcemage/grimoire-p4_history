install  -m  755  bin/*  ${INSTALL_ROOT}/usr/X11R6/bin     &&
install  -m  755  lib/*  ${INSTALL_ROOT}/usr/lib           &&

# Install documentation...
D_DIR="${INSTALL_ROOT}/usr/share/doc/${SPELL}/${VERSION}"  &&
install  -d  -m  755  ${D_DIR}            		   &&
cd  ${D_DIR}            		                   &&
unpack_file  5
