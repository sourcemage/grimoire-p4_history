   SOURCE_URL[0]=http://www.apache.org/dist/${PKG_PROJECT}/${PKG_SUBPRJ}/${PKG_NAME}/source/$SOURCE
          MD5[0]=0908b7d166f70961633991e7c001f35a
      LICENSE[0]=APACHE
        WEB_SITE=http://jakarta.apache.org/commons/collections.html
         ENTERED=20030326
