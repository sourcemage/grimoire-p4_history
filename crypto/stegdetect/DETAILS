           SPELL=stegdetect
         VERSION=0.6
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://www.outguess.org/$SOURCE
          MD5[0]=a4a78ce569d43a1dd593d4b0fa567b8a
        WEB_SITE=http://www.outguess.org
         ENTERED=20020116
