           SPELL=gpasman
         VERSION=1.9.3
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://gpasman.sourceforge.net/files/$SOURCE
     SOURCE_HASH=sha512:4e7bef90123fc25aeeaaf0970db771d8380e00e82ffce60c16b026d0c6876ae3caa2f019eb10977872de1c659779529222151d2cca1a0ba85c6e0648c89973f2
