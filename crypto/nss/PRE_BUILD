mk_source_dir                   &&
unpack  ${SOURCE}    ${MD5[0]}  &&
cd $SOURCE_DIRECTORY            &&
unpack  ${SOURCE2}   ${MD5[0]}  &&
unpack  ${SOURCE3}   ${MD5[0]}  &&
cp -a  nsprpub-4.3.0/*  .         &&
rm -rf nsprpub-4.3.0/             &&
cp -a  dbm-4.3.0/* .              &&
rm -rf dbm-4.3.0  

