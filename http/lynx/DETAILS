           SPELL=lynx
         VERSION=2.8.5
          SOURCE=$SPELL$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${SPELL}2-8-5
   SOURCE_URL[0]=http://lynx.isc.org/release/$SOURCE
     SOURCE_HASH=sha512:d5fd6c560d76d9df0aefe3eaedbc422669993f2210a6139c1c9249b9d2c110b4bc797f75ba0ed80f66d54ae7ad96159067fe80e6e25bcc42d0961ea1e2f59b5d
