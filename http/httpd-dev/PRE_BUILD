# $Id: PRE_BUILD,v 1.3 2003/01/07 19:02:09 sergeyli Exp $
mk_source_dir $SOURCE_DIRECTORY                  &&
unpack        $SOURCE                  ${MD5[0]} &&
cd            $SOURCE_DIRECTORY/srclib           &&
unpack        $SOURCE2                 ${MD5[1]} &&
mv            ${SOURCE2%%\.tar\.*}     apr       &&
unpack        $SOURCE3                 ${MD5[2]} &&
mv            ${SOURCE3%%\.tar\.*}     apr-util
