# $Id: PRE_BUILD,v 1.2 2002/09/17 15:45:42 sergeyli Exp $
mk_source_dir $SOURCE_DIRECTORY                  &&
unpack        $SOURCE                  ${MD5[0]} &&
cd            $SOURCE_DIRECTORY/srclib           &&
unpack        $SOURCE2                 ${MD5[1]} &&
mv            ${SOURCE2%%\.*}          apr       &&
unpack        $SOURCE3                 ${MD5[2]} &&
mv            ${SOURCE3%%\.*}          apr-util
