if  [  "$CVS"  ==  "y"  ];  then
  mk_source_dir                          &&
  cd  $SOURCE_DIRECTORY                  &&
  unpack  $SOURCE   ${MD5[0]}            &&
  unpack  $SOURCE2  ${MD5[1]}            &&
  rm  -rf  */CVS                         &&
  mv  oscommerce-catalog-cvs/*  .        &&
  mv  oscommerce-admin-cvs/*    catalog  &&
  rmdir  oscommerce-admin-cvs            \
         oscommerce-catalog-cvs
else
  default_pre_build
fi
