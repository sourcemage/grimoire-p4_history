#!/bin/bash

RUNLEVEL=S

. /etc/init.d/smgl_init

case $1 in
  start)    if [ -f /etc/sysctl.conf ] ; then
              echo "Running sysctl..."
              loadproc /sbin/sysctl -p
            fi
            ;;

  stop)     exit 0
            ;;

  *)        echo "Usage: $0 {start|stop}"
            exit 1
            ;;
esac
