OPTS="$OPTS --enable-all-optimizations    \
            --enable-fpu                  \
            --with-x11                    \
            --enable-ne2000               \
            --enable-cdrom=yes            \
            --enable-icache               \
            --enable-repeat-speedups      \
            $RFB $SB16 $TERM $APIC $SMP $IPS"  &&

