           SPELL=qemu
         VERSION=0.7.2
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://fabrice.bellard.free.fr/qemu/$SOURCE
     SOURCE_HASH=sha512:f71bab851b11874647ef6c2731ee75249f0cf6e8ad325002c788f195367a6c5e3510d4fe17149e73e2e7531c912271632b197f1d7c22fd15b3bd7ce09ecb73f4
