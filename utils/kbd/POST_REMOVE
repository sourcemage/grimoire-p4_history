if  [  !  -f   /etc/init.d/sunfont.sh  ];  then
  rm -f /etc/init.d/sunfont.sh
  for runlev in 2 3 4 5; do
    rm -f /etc/rc${runlev}.d/S80sunfont
  done
fi


if  [  !  -f   /etc/init.d/log2cons.sh  ];  then
  rm -f /etc/init.d/log2cons.sh
  for runlev in 2 3 4 5; do
    rm -f /etc/rc${runlev}.d/S10log2cons
  done
fi


if  [  !  -f   /usr/bin/logcolorise.pl ];  then
  rm -f /usr/bin/logcolorise.pl
fi

if  [  !  -f   /usr/bin/tailfmessages ];  then
  rm -f /usr/bin/tailfmessages
fi
