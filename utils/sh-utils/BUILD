default_build  &&  (

  mv  -f  /usr/bin/date      /bin
  mv  -f  /usr/bin/echo      /bin
  mv  -f  /usr/bin/false     /bin
  mv  -f  /usr/bin/groups    /bin
  mv  -f  /usr/bin/pwd       /bin
  mv  -f  /usr/bin/stty      /bin
  mv  -f  /usr/bin/true      /bin
  mv  -f  /usr/bin/uname     /bin
  rm  -f  /usr/bin/hostname
  rm  -f  /usr/bin/su

)
