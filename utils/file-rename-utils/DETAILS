           SPELL=file-rename-utils
         VERSION=1.1
          SOURCE=$SPELL-$VERSION.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL
   SOURCE_URL[0]=ftp://ibiblio.org/pub/linux/utils/file/$SOURCE
          MD5[0]='9c3871e672cb53ca48eb24676af4636c'
     GATHER_DOCS=off
        WEB_SITE=http://freshmeat.net/projects/file-rename-utils/
         ENTERED=20040409
