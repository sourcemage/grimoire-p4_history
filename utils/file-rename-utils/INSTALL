  cp -af \
    lowercase \
    mv_lowercase \
    mv_uppercase \
    mv_capitalize \
    mv_spaces2underscore \
    mv_underscore2hyphen \
    mv_chars2null \
    mv_chars2underscore \
    mv_number \
    mv_number.ext \
    mv_prefix \
    mv_sed \
    mv_suffix \
    htmlrename \
    Trash \
    swap                      /usr/bin/ &&
  cp -f \
    lowercase.1 \
    htmlrename.1 \
    Trash.1 \
    swap.1                    /usr/share/man/man1

