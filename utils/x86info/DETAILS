          SOURCE=$SPELL-$VERSION.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://www.codemonkey.org.uk/projects/x86info/$SOURCE
     SOURCE_HASH=sha1:31bc6bb1403e867f37b14c22a63008257f8e8c31
