           SPELL=x86info
         VERSION=1.10
          SOURCE=$SPELL-$VERSION.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://www.codemonkey.org.uk/x86info/$SOURCE
          MD5[0]='6f9d52982c11621e63a6a7874db67105'
        WEB_SITE=http://www.codemonkey.org.uk/
         ENTERED=20011122
