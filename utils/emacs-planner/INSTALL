if [ ! -d ${INSTALL_ROOT}/usr/share/emacs/site-lisp/planner ]
then
    mkdir ${INSTALL_ROOT}/usr/share/emacs/site-lisp/planner
fi

install -m0644 *.el ${INSTALL_ROOT}/usr/share/emacs/site-lisp/planner &&
install -m0644 *.info /usr/share/info                                 &&

install-info --info-dir=${INSTALL_ROOT}/usr/share/info \
        ${INSTALL_ROOT}/usr/share/info/planner-el.info
