           SPELL=lsof
         VERSION=4.75
          SOURCE=${SPELL}_${VERSION}.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${SPELL}_$VERSION
   SOURCE_URL[0]=ftp://lsof.itap.purdue.edu/pub/tools/unix/lsof/$SOURCE
          MD5[0]='b37eb255804eafcb5fa43e43798249d0'
