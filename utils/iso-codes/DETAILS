           SPELL=iso-codes
         VERSION=0.48
          SOURCE=${SPELL}_$VERSION-1.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://ftp.debian.org/debian/pool/main/i/iso-codes/$SOURCE
        WEB_SITE=http://alioth.debian.org/projects/pkg-isocodes/
      SOURCE_GPG="gurus.gpg:${SOURCE}.sig"
         ENTERED=20050726
         UPDATED=20050925
      LICENSE[0]=LGPL
       BUILD_API=2
           SHORT='Language and Territory names and codes by ISO'
cat << EOF
This package provides the ISO-639 Language code list, the ISO-3166 Territory
code list, and ISO-3166-2 sub-territory lists, and all their translations in
gettext .po form.
EOF
