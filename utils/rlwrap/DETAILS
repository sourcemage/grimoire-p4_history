           SPELL=rlwrap
         VERSION=0.21
       BUILD_API=2
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://utopia.knoware.nl/~hlub/uck/rlwrap/$SOURCE
          MD5[0]=1355fcc2498160829b98dc5cdf6345c0
