           SPELL=pear-zip
    PEAR_PACKAGE=zip
         VERSION=1.0
          MD5[0]='608f438574f72786105359465477737c'
         UPDATED=20050408
      LICENSE[0]=PHP
           SHORT='PEAR: A zip management extension'
cat << EOF
