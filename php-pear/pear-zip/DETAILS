           SPELL=pear-zip
    PEAR_PACKAGE=zip
         VERSION=1.0
     SOURCE_HASH='sha512:bd6c6aa6be5954898009939c220317fae8256bb38c008f7a87a4aaf29deba6d46fa1e806bf53b7d00ea66a2aeb70add767b36475ee80cc361f35b304dec79102'
