           SPELL=solfege
         VERSION=2.1.3
          SOURCE=$SPELL-easybuild-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-easybuild-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
        WEB_SITE=http://solfege.sf.net
         ENTERED=20031206
