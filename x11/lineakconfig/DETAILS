           SPELL=lineakconfig
         VERSION=0.3
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://telia.dl.sourceforge.net/sourceforge/lineak/$SOURCE
   SOURCE_URL[1]=http://unc.dl.sourceforge.net/sourceforge/lineak/$SOURCE
