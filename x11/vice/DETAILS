           SPELL=vice
         VERSION=1.9
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=ftp://ftp.funet.fi/pub/cbm/crossplatform/emulators/VICE/$SOURCE
     SOURCE_HASH=sha512:b86fde10e8b8d56d89a6e82a425f851dfd5e6b649b705e0f5468b23e739a08672f781c14a957e5a7e8a3ce2dbfdf5c9c050ec0dbabd9759877305c41df703ddc
