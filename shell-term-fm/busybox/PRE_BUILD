default_pre_build      &&
cd  $SOURCE_DIRECTORY  &&
if use_gcc 4.0
then
	patch -p0 <$SCRIPT_DIRECTORY/gcc-4.0-gzip.c.patch
fi &&

if  [  -f  $CONFIG_CACHE/busybox.config  ];  then
  cp  ${CONFIG_CACHE}/busybox.config  .config
else
  cp  ${SCRIPT_DIRECTORY}/busybox.config  .config  
fi  &&

if  [  "$BB_CONFIG"  =  "y"  ];  then
  VOYEUR="on"         &&
  make  "menuconfig"
fi  &&
  
cp  .config  ${CONFIG_CACHE}/busybox.config
