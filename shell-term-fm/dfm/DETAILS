           SPELL=dfm
         VERSION=0.99.9
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL
   SOURCE_URL[0]=http://www.kaisersite.de/dfm/$SOURCE
        WEB_SITE=http://www.kaisersite.de/dfm/
      BUILD_API=2 
         ENTERED=20011130
