# `/etc/zshenv' is sourced on all invocations of the shell,
# unless the -f option is set.  It should contain commands to set
# the command search path, plus other important environment variables.
# .zshenv should not contain commands that product output or assume
# the shell is attached to a tty.

path=(/sbin /usr/sbin /bin /usr/bin /usr/local/bin $path)

export TMP=/tmp
export TMPDIR=/tmp
