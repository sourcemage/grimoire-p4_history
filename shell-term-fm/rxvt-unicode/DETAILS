SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://rxvt-unicode-dist.plan9.de/$SOURCE
        WEB_SITE=http://sourceforge.net/projects/rxvt-unicode
