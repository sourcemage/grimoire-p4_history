(
mkdir  ggi  ggi/demos  ggi/ggiglut                     	&&
touch  ggi/demos/Makefile.in  ggi/ggiglut/Makefile.in  	&&
default_build						&&
# Install SGI's GLU 1.3 implementation (Mesa contains 1.1/1.2 only)
cd  ${INSTALL_ROOT}/usr/lib                   		&&
unpack  ${SOURCE2}  ${MD5[1]}

)  >  ${C_FIFO}  2>&1

