   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
   SOURCE_URL[1]=http://www.ibiblio.org/pub/linux/system/emulators/$SPELL/$SOURCE
        WEB_SITE=http://www.winehq.com/
